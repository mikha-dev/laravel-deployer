{
    "name": "mikha-dev/laravel-deployer",
    "description": "A fork of Lorisleiva, Laravel Deployer improved to support php8 giving Artisan the power of zero-downtime deployment.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeremie ALVIN",
            "email": "contact@jeremiealvin.com"
        },
        {
            "name": "Loris LEIVA",
            "email": "loris.leiva@gmail.com"
        }
    ],
    "require": {
        "deployer/deployer": "^7.0.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0"
    },
    "autoload": {
       "psr-4": {
            "Lorisleiva\\LaravelDeployer\\": "src/LaravelDeployer"
        },
        "files": [
            "src/autoload.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Lorisleiva\\LaravelDeployer\\Test\\": "tests"
        },
        "files": [
            "tests/autoload.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Lorisleiva\\LaravelDeployer\\LaravelDeployerServiceProvider"
            ]
        }
    }
}
